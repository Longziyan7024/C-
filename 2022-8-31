//switch和default的相关应用
{
	int day = 0;
		scanf("%d", &day);
	switch (day) 
	{
		case 1:
		case 2:
		case 4:
		case 5:
		printf("工作日\n");
		break;
		case 6:
		case 7:
		printf("休息日\n");
		break;
		default:
		printf("没有此日期\n");
		break;
	}
}
